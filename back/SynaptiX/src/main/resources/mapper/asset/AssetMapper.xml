<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.asset.AssetMapper">
    <select id="selectAsset" resultType="com.example.demo.model.AssetDTO">
        SELECT 
            TotalAssets AS totalAssets,
            CurrenAssets AS currentAssets,
            TotalEarning AS totalEarning,
            TotalCost AS totalCost
        FROM assets
        LIMIT 1
    </select>
</mapper>
